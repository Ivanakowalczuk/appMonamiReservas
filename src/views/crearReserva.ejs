<html lang="es">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">

    <title>Nueva Reserva</title>

  

    <%- include('partials/headDashboard') %>


    
  
  </head>
  <body>

    <%- include ('partials/headerDashboard')%>

<div class="container-fluid mb-5">
  <div class="row">
    <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse">
      <div class="position-sticky pt-3">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="dashboard">
              <span data-feather="home" class="align-text-bottom"></span>
             Reservas
            </a>
          </li>
          
          </li>
          <li class="nav-item">
            <a class="nav-link" href="dashboardSquash">
              <span  class="align-text-bottom"></span>
              Squash 
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="dashboardPadel">
              <span  class="align-text-bottom"></span>
              Padel
            </a>
          </li>  
          <li class="nav-item">
            <a class="nav-link" href="dashboardCliente">
              <span data-feather="bar-chart-2" class="align-text-bottom"></span>
            Clientes
            </a>
          </li>
         
        </ul>
      </div>
    </nav>

    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
        <section class="d-flex flex-column align-items-center justify-content-center my-4  ">
           <h1>Nueva reserva</h1>
           <article class="reservaCancha">
            <div class="card" >
             <div class="d-flex justify-content-center align-middle ">
               
                 <img src="../img/padel1.jpg" class="img-fluid rounded imgDeporte" alt="padel">
                      
                  <div class="card-body">
                    
                    
                  <form class="d-flex flex-column justify-content-center align-items-center form-reserva" action="reservarPadel"  method="post">                      
                      <input class="dateForm" data-provider="datapicker" type="date" name="fecha" id="fecha" required >
                     
                                  
                  <select name="cancha" id="cancha" class="my-2 dateForm">
                    <option desable>Elige una cancha</option>
                    <option value="p1">Padel - Cancha 1</option>
                    <option value="p2">Padel - Cancha 2</option>
                    <option value="s1">Squash - Cancha 1</option>
                    <option value="s2">Squash - Cancha 2</option>
                    <option value="s3">Squash - Cancha 3</option>
                  </select>
                     
                     
                   <select class="dateForm m-3 scroll"  name= "hora_inicio" id="hora_inicio" required> 
                   
                    <option class="hora " type="time" value="" selected  disabled>Hora de inicio</option>
                     <option class="hora" type="time" value="900"><%=horas[0]%></option>
                     <option class="hora" type="time" value="930"><%=horas[1]%></option>
                     <option class="hora" type="time" value="960"><%=horas[2]%></option>
                     <option class="hora" type="time" value="990"><%=horas[3]%></option>
                     <option class="hora" type="time" value="1020"><%=horas[4]%></option>
                     <option class="hora" type="time" value="1050"><%=horas[5]%></option>
                     <option class="hora" type="time" value="1080"><%=horas[6]%></option>
                     <option class="hora" type="time" value="1110"><%=horas[7]%></option>
                     <option class="hora" type="time" value="1140"><%=horas[8]%></option>
                     <option class="hora" type="time" value="1170"><%=horas[9]%></option>
                     <option class="hora" type="time" value="1200"><%=horas[10]%></option>
                     <option class="hora" type="time" value="1230"><%=horas[11]%></option>
                     <option class="hora" type="time" value="900"><%=horas[12]%></option>
                     <option class="hora" type="time" value="930"><%=horas[13]%></option>
                     <option class="hora" type="time" value="960"><%=horas[14]%></option>
                     <option class="hora" type="time" value="990"><%=horas[15]%></option>
                     <option class="hora" type="time" value="1020"><%=horas[16]%></option>
                     <option class="hora" type="time" value="1050"><%=horas[17]%></option>
                     <option class="hora" type="time" value="1080"><%=horas[18]%></option>
                     <option class="hora" type="time" value="1110"><%=horas[19]%></option>
                     <option class="hora" type="time" value="1140"><%=horas[20]%></option>
                     <option class="hora" type="time" value="1170"><%=horas[21]%></option>
                     <option class="hora" type="time" value="1200"><%=horas[22]%></option>
                     <option class="hora" type="time" value="1230"><%=horas[23]%></option>
                     <option class="hora" type="time" value="1260"><%=horas[24]%></option>
                     <option class="hora" type="time" value="1290"><%=horas[25]%></option>
                     <option class="hora" type="time" value="1320"><%=horas[26]%></option>
                     <option class="hora" type="time" value="1350"><%=horas[27]%></option>
                     <option class="hora" type="time" value="1380"><%=horas[28]%></option>
                     
                   </select>
                   <select class="m-3 dateForm "  name= "duracion" id="duracion" required> 
                     <option selected disabled>Duraci√≥n</option>
                     <option class="hora" type="time" value="60" requiered >60 min</option>
                     <option class="hora" type="time" value="90">90 min</option>
                     <option class="hora" type="time" value="120">120min</option>
                   </select>

                  <a><button type="submit" class="btn btn-outline-secondary">Enviar</button></a>
                                
                 </div>
                 <img src="../img/squash1.jpg" class="img-fluid rounded imgDeporte" alt="padel"> 
             </div>
           </div>
           </form>
         </article>     
          
               
          
          
    
           </section>
    </main>
  </div>
</div>


